<!DOCTYPE html>
<html lang="en">

<head>
    <% include ../partials/head %>
</head>

<body>
    <% include ../partials/nav %>
    <h1>Matches</h1>

    <% for (let [key, value] of Object.entries(matches)) {%>
        <% console.log({key, value}) %>
        <p><%= value.name %></p>
        <p>age: <%= value.age %></p>
        <p class="macthLocation"><%= value.location %>km</p>
        <img src="<%= value.image %>" alt="a picture of <%= value.name %>">
    <% } %>

    <% include ../partials/footer %>
</body>

</html>